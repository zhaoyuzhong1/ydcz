                    <div class="car-name"><b>${car.title}</b> <span
                            class="feature-tags"></span></div>

                    <c:choose>
                        <c:when test="${car.type=='0'}">
                            <div class="car-attributes">
                                <div class="left-texts">
                                    <div class="color">${car.color}</div>
                                    <div class="guide-price">
                                        <div class="price-text">实际售价: ${car.price}万</div>
                                        <img src="${ctx}/images/help.png" class="price-question"></div>
                                </div>
                                <div class="sold-number"><span>已售${car.sailnum}台</span></div>
                            </div>
                        </c:when>
                        <c:when test="${car.type=='1'}">
                            <div class="car-attributes">
                                <div class="left-texts">
                                    <div class="color">${car.color}</div>
                                    <div class="guide-price">
                                        <div class="price-text">实际售价: ${car.price}万</div>
                                        <img src="${ctx}/images/help.png" class="price-question"></div>
                                </div>
                                <div class="sold-number"><span>已行驶${car.km}公里</span></div>
                            </div>
                        </c:when>
                        <c:otherwise>
                            <div class="car-attributes">
                                <div class="left-texts">
                                    <div class="color">${car.color}</div>
                                    <div class="guide-price">
                                        <div class="price-text">实际售价: ${car.price}万</div>
                                        <img src="${ctx}/images/help.png" class="price-question"></div>
                                </div>
                                <div class="sold-number"><span>已参团${car.tcase}</span></div>
                            </div>
                        </c:otherwise>
                    </c:choose>
